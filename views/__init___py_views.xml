<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View __init__.py View Tree -->
    <record id="view___init___py_tree" model="ir.ui.view">
        <field name="name">view.__init__.py.tree</field>
        <field name="model">__init__.py</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!-- View __init__.py form -->
    <record id="view___init___py_form" model="ir.ui.view">
        <field name="name">view.__init__.py.form</field>
        <field name="model">__init__.py</field>
        <field name="arch" type="xml">
            <form string="__init__ Py">
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1>
                            <field name="name" placeholder="Name..."/>
                        </h1>
                    </div>
                </sheet>
            </form>
        </field>
    </record>

    <!-- View __init__.py search -->
    <record id="view___init___py_search" model="ir.ui.view">
        <field name="name">view.__init__.py.search</field>
        <field name="model">__init__.py</field>
        <field name="arch" type="xml">
            <search>
                <group expand="1" string="Group By">
                    <filter string="Name" name="name" domain="[]" context="{'group_by':'name'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Action __init__.py -->
    <record id="action___init___py" model="ir.actions.act_window">
        <field name="name">__init__ Py</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">__init__.py</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                There is no examples click here to add new __init__ Py.
            </p>
        </field>
    </record>

</odoo>
